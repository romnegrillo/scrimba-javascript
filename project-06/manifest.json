{
    "manifest_version": 3,
    "version": "1.1",
    "name": "Bookmark",
    "action": {
        "default_popup": "index.html",
        "default_icon": "bookmark-16.png"
    },

    "icons": { "16": "bookmark-16.png",
        "48": "bookmark-48.png",
       "128": "bookmark-128.png" },

    "permissions": [
        "tabs"
    ]
}